{
  "name": "pulselocker-website",
  "version": "3.0.0",
  "description": "Pulselocker Website",
  "license": "private",
  "scripts": {
    "lint": "standard",
    "start": "babel-node src/server/main.js",
    "test": "jest --coverage",
    "validate": "npm t && npm run lint"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^20.0.4",
    "pre-commit": "^1.2.2",
    "standard": "^10.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "pre-commit": {
    "run": [
      "test",
      "lint"
    ],
    "silent": true
  }
}
